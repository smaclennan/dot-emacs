(defvar errno-strings
  '("SUCCESS"
    "EPERM"
    "ENOENT"
    "ESRCH"
    "EINTR"
    "EIO"
    "ENXIO"
    "E2BIG"
    "ENOEXEC"
    "EBADF"
    "ECHILD"
    "EAGAIN/EWOULDBLOCK"
    "ENOMEM"
    "EACCES"
    "EFAULT"
    "ENOTBLK"
    "EBUSY"
    "EEXIST"
    "EXDEV"
    "ENODEV"
    "ENOTDIR"
    "EISDIR"
    "EINVAL"
    "ENFILE"
    "EMFILE"
    "ENOTTY"
    "ETXTBSY"
    "EFBIG"
    "ENOSPC"
    "ESPIPE"
    "EROFS"
    "EMLINK"
    "EPIPE"
    "EDOM"
    "ERANGE"
    ))

;;;###autoload
(defun errno-string (errno)
  (interactive "Nerrno: ")
  (let ((str (nth errno errno-strings)))
    (if str
	(message "%s" str)
      (error "Not found"))))

(provide 'errno)
